    "import turtle\n",
    "\n",
    "t = turtle.Turtle()\n",
    "\n",
    "t.speed(100)\n",
    "turtle.bgcolor(\"black\")\n",
    "\n",
    "for i in range(100):\n",
    "    t.color(\"cyan\")\n",
    "    t.circle(i)\n",
    "    t.left(5)\n",
    "    \n",
    "turtle.done()\n"
